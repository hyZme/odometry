<?xml version="1.0"?>

<robot name="mobile_robot" xmlns:xacro="http://ros.org/wiki/xacro">
    <xacro:include filename="$(find odometry)/urdf/properties.xacro"/>

    <link name="base_footprint"></link>

    <joint name ="base_joint" type="fixed">
        <parent link="base_footprint"/>
        <child link="base_link"/>
        <origin xyz="0.0 0.0 0.10" rpy="0 0 0"/>        
    </joint>

    <link name="base_link"> 
        <visual>
            <origin xyz="0 0 0.04" rpy="0.0 0.0 0.0" />
            <geometry>
                <box size="0.5 0.3 0.08"/>
            </geometry>
            <material name="blue">
                <color rgba="0.0 0.5 1 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0.12" rpy="0.0 0.0 0.0" />
            <geometry>
                <box size="0.5 0.3 0.08"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="1"/>
            <inertia ixx="0.008" ixy="0.0" ixz="0.0" iyy="0.02136" iyz="0.0" izz="0.0215"/>
        </inertial>
    </link>
    
    <joint name="caster_back_joint" type="fixed">
        <parent link="base_link"/>
        <child link="caster_back_link"/>
        <origin xyz="-0.18 0 0" rpy="0 0 0"/>
    </joint>

    <link name="caster_back_link">
        <visual name="caster">
            <origin xyz="0.0 0 -0.05"/>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
        </visual>
        <collision>
          <origin xyz="0 0 -0.05" />
          <geometry>
            <sphere radius="0.05"/>
          </geometry>
        </collision>
    
        <inertial>
          <origin xyz="0 0 0" />
          <mass value="0.005" />
          <inertia ixx="0.001" ixy="0.0" ixz="0.0"
                   iyy="0.001" iyz="0.0"
                   izz="0.001" />
        </inertial>
    </link>



    <joint name="joint_right_wheel" type="continuous"> 
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <origin xyz="0.1 -0.185 0.0" rpy="-1.570795 0 0"/>
        <axis xyz="0.0 0 1"/>
    </joint>

    <link name="right_wheel"> 
        <visual>
            <geometry>
                <cylinder radius="0.1" length="0.07"/>
            </geometry>
            <material name="black">
                <color rgba="0.05 0.05 0.05 1"/>
            </material>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.1" length="0.07"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.3"/>
            <inertia ixx="0.00145" ixy="0.0" ixz="0.0" iyy="0.00145" iyz="0.0" izz="0.0025"/>
        </inertial>
    </link>

    
    <joint name="joint_left_wheel" type="continuous"> 
